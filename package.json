{
  "name": "anime-dl",
  "version": "1.0.0",
  "description": "Automatic anime torrentor",
  "author": "Neil Mehra <neilmehra@outlook.com>",
  "main": "src/main.js",
  "repository": "https://github.com/AsianKoala/anime-dl",
  "scripts": {
    "start": "electron .",
    "build": "run-os",
    "test": "echo success",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "neilmehra.anime-dl",
    "productName": "anime-dl",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "portable"
      ],
      "icon": "build/azusa_build.ico"
    },
    "portable": {
      "artifactName": "anime-dl.exe"
    }
  },
  "devDependencies": {
    "electron": "^13.1.7",
    "electron-builder": "^22.11.7",
    "electron-is-dev": "^2.0.0",
    "rimraf": "^3.0.2",
    "run-script-os-fix": "^1.0.4"
  },
  "dependencies": {
    "@electron/remote": "^1.2.1",
    "axios": "^0.21.1"
  }
}